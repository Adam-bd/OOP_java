<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox alignment="CENTER" spacing="20.0" xmlns="http://javafx.com/javafx/17.0.12" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.guiclient.HelloController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
   <TextField fx:id="loginTextField" />
   <Button fx:id="loginButton" onAction="#onLoginButtonClick" mnemonicParsing="false" text="Login" />
   <TextArea fx:id="outputTextArea" prefHeight="200.0" prefWidth="200.0" />
   <HBox prefHeight="100.0" prefWidth="200.0">
      <children>
         <TextField fx:id="inputTextField" onAction="#onSendButtonClick" />
         <Button fx:id="sendButton" mnemonicParsing="false" onAction="#onSendButtonClick" text="Send" />
      </children>
   </HBox>

    <Label text="online" />
   <ListView fx:id="onlineListView" prefHeight="200.0" prefWidth="200.0" />
</VBox>
